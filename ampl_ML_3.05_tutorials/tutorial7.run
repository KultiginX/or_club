#RESET THE AMPL ENVIROMENT
reset;

#LOAD THE MODEL
model tutorial7.mod;

#LOAD THE DATA
data tutorial7.dat;

#DISPLAY THE PROBLEM FORMULATION
expand total_cost, supply_constraint, demand_constraint;

#CHANGE THE SOLVER (optional)
option solver cplex;

#SOLVE
solve;

#SHOW RESULTS
display x, total_cost;